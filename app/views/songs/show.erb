<h1><%= @song.name %></h1>
<h2><%= @song.artist.name %></h2>
    <a href="/artists/<%= @song.artist.slug %>">Go To Artist</a>
    <% @song.genres.each do |genre| %>
    <h2><%= genre.name %></h2>
    <a href="/genres/<%= genre.slug %>">Go To Genre</a>
    <% end %>
    <a href="/songs/<%= @song.slug %>/edit">Edit</a>


<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>